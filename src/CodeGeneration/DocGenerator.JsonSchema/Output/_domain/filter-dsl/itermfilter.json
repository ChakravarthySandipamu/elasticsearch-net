{
  "id": "Nest.ITermFilter",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "ITermFilter",
  "type": "object",
  "properties": {
    "boost": {
      "type": "number"
    },
    "_cache": {
      "type": "boolean"
    }
  },
  "patternProperties": {
    "^(?!boost$|_cache$).*": {
      "type": [ "string", "number", "boolean" ]
    }
  }
}
